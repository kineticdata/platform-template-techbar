        {
          "name": "Build Input Table",
          "type": "Load",
          "action": "Custom",
          "code": "if (K('field[Inputs JSON]').value() !== null){\n  var data = JSON.parse(K('field[Inputs JSON]').value());\n\t\n\t// build the table from JSON\n  $.each(Object.keys(data),function(){\n\t\tlabel = $('<label for=\"input_' + this + '\" class=\"control-label col-3\">' + this + '</label>');\n\t\tvalue = $('<div class=\"col-9\">').append($('<input type=\"text\" id=\"input_' + this + '\" class=\"form-control required\">').val(data[this] || \"\"))\n    wrapper = $('<div class=\"form-group\">').append(label,value);\n    $('#inputs').append(wrapper);\n\t})\n}"
        },
        {
          "name": "Update Outputs",
          "type": "Submit",
          "action": "Custom",
          "code": "var data = {}\n$('#inputs .form-group').each(function(){\n\tlabel = $(this).find('label').first().text();\n\tvalue = $(this).find('input').first().val() || null;\n\tdata[label]=value;\n});\nK('field[Inputs JSON]').value(JSON.stringify(data));",
          "runIf": "values('Inputs Updated') !== 'Retry'"
        }
